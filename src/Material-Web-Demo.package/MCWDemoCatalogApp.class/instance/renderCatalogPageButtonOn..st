        <button class="demo-button mdc-button mdc-button--dense mdc-button--raised mdc-ripple-upgraded">Dense</button>
        <button class="demo-button mdc-button mdc-button--raised mdc-ripple-upgraded"><i class="material-icons mdc-button__icon">favorite</i>Icon</button>
    </div>
    <div>
        <h3 class="mdc-typography--subtitle1">Unelevated Button</h3>
        <button class="demo-button mdc-button mdc-button--unelevated mdc-ripple-upgraded">Default</button>
        <button class="demo-button mdc-button mdc-button--dense mdc-button--unelevated mdc-ripple-upgraded">Dense</button>
        <button class="demo-button mdc-button mdc-button--unelevated mdc-ripple-upgraded"><i class="material-icons mdc-button__icon">favorite</i>Icon</button>
